{
  "description": "WebExtension for TouchpadGesture Advanced",
  "manifest_version": 2,
  "name": "WebExtension for TouchpadGesture Advanced",
  "version": "0.1.1",
  "icons": {
    "48": "icons/TGA_icon_48.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "we_f_tga@techchair.net",
      "strict_min_version": "60.0"
    }
  },
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icons/TGA_icon_48.png"
  },
  "permissions": [
    "nativeMessaging",
    "tabs",
    "storage",
    "<all_urls>"
  ]
}