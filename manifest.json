{
  "description": "WebExtension for TouchpadGesture Advanced",
  "manifest_version": 2,
  "name": "WebExtension for TouchpadGesture Advanced",
  "version": "1.0",
  "icons": {
    "48": "icons/TGA_icon_48.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "ping_pong@example.org",
      "strict_min_version": "50.0"
    }
  },
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icons/TGA_icon_48.png"
  },
  "permissions": [
    "nativeMessaging",
    "tabs",
    "storage",
    "<all_urls>"
  ]
}